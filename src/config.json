{
  "name": "RawNet2+Hope",
  "n_gpu": 0,
  "preprocessing": {
      "spectrogram": {
          "args": {
            "sr": 22050,
            "win_length": 1024,
            "hop_length": 256,
            "n_fft": 1024,
            "f_min": 0,
            "f_max": 8000,
            "n_mels": 80,
            "power": 1.0,
            "pad_value": -11.5129251
          }
      },
      "log_spec": false
  },
  "arch": {
      "type": "RawNet2",
      "args": {}
  },
  "data": {
      "train": {
          "batch_size": 32,
          "num_workers": 5,
          "datasets": [
              {
                  "type": "ASVDataset",
                  "args": {
                      "part": "train",
                      "segment_size": 64000
                  }
              }
          ]
      },
      "val": {
          "batch_size": 1,
          "num_workers": 5,
          "datasets": [
            {
                "type": "ASVDataset",
                "args": {
                    "part": "dev",
                    "segment_size": 64000
                }
            }
          ]
      },
      "test": {
          "batch_size": 1,
          "num_workers": 5,
          "datasets": [
            {
                "type": "ASVDataset",
                "args": {
                    "part": "eval",
                    "segment_size": 64000
                }
            }
          ]
      }
  },
  "generator_optimizer": {
      "type": "AdamW",
      "args": {
        "lr": 0.0002,
        "betas": [0.8, 0.99],
        "weight_decay": 0.01,
        "eps": 1e-9
      }
  },
  "discriminator_optimizer": {
    "type": "AdamW",
    "args": {
      "lr": 0.0002,
      "betas": [0.8, 0.99],
      "weight_decay": 0.01,
      "eps": 1e-9
    }
  },
  "generator_loss": {
      "type": "GeneratorLoss",
      "args": {
        "lambda_fm": 2,
        "lambda_mel": 45
      }
  },
  "discriminator_loss": {
    "type": "DiscriminatorLoss",
    "args": {}
  },
  "metrics": [
      {}
  ],
  "generator_lr_scheduler": {
    "type": "ExponentialLR",
    "args": {
        "gamma": 0.999
    }
  },
  "discriminator_lr_scheduler": {
    "type": "ExponentialLR",
    "args": {
        "gamma": 0.999
    }
  },
  "trainer": {
      "epochs": 50,
      "save_dir": "saved/",
      "save_period": 5,
      "verbosity": 2,
      "monitor": "off",
      "early_stop": 50,
      "visualize": "wandb",
      "wandb_project": "as_project",
      "len_epoch": 500
  }
}